@article{botev2007generalized,
author = {Z.I. Botev and D.P. Kroese and T. Taimre},
title ={Generalized Cross-entropy Methods with Applications to Rare-event Simulation and Optimization},
journal = {SIMULATION},
volume = {83},
number = {11},
pages = {785-806},
year = {2007},
doi = {10.1177/0037549707087067},
abstract = { The cross-entropy and minimum cross-entropy methods are well-known Monte Carlo simulation techniques for rare-event probability estimation and optimization. In this paper, we investigate how these methods can be eXtended to provide a general non-parametric cross-entropy framework based on φ-divergence distance measures. We show how the χ2 distance, in particular, yields a viable alternative to the Kullback—Leibler distance. The theory is illustrated with various eXamples from density estimation, rare-event simulation and continuous multi-eXtremal optimization. }
}




